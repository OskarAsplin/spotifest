{
  "name": "spotifest",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "^5.0.0-alpha.92",
    "@mui/material": "^5.9.2",
    "@mui/styles": "^5.9.2",
    "@mui/x-date-pickers": "^5.0.0-beta.4",
    "@tanstack/react-query": "5.0.0",
    "@tanstack/react-query-devtools": "5.0.0",
    "@tanstack/router": "0.0.1-beta.125",
    "@tanstack/router-devtools": "0.0.1-beta.125",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/async": "^3.2.5",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^24.9.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.19.16",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-image-gallery": "^1.2.0",
    "@types/react-swipeable-views": "^0.13.0",
    "copy-to-clipboard": "^3.3.3",
    "countries-list": "^2.6.1",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "i18next": "^22.0.8",
    "lodash-es": "^4.17.21",
    "madge": "^6.0.0",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.0.3",
    "react-cookie-consent": "^5.2.0",
    "react-country-flag": "^2.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^12.0.0",
    "react-image-gallery": "^1.2.11",
    "react-intl": "^4.7.6",
    "react-player": "2.12.0",
    "react-share": "^4.4.1",
    "react-swipeable-views": "^0.13.9",
    "sass": "^1.32.6",
    "spotify-web-api-js": "^0.22.1",
    "typescript": "5.1.6",
    "zustand": "^4.3.9"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "find-circular-dependencies": "madge --circular ./src/index.tsx",
    "find-unused-exports": "ts-prune",
    "find-dead-code": "ts-prune | grep -v \"(used in module)\" || true"
  },
  "type": "module",
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "madge": {
    "detectiveOptions": {
      "tsx": {
        "skipTypeImports": true
      },
      "ts": {
        "skipTypeImports": true
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.1.0",
    "@storybook/addon-links": "7.1.0",
    "@storybook/addon-styling": "1.3.4",
    "@storybook/builder-vite": "7.1.0",
    "@storybook/react": "7.1.0",
    "@storybook/react-vite": "7.1.0",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromatic": "^6.11.4",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.13",
    "prettier": "3.0.0",
    "prop-types": "^15.8.1",
    "storybook": "7.1.0",
    "storybook-react-i18next": "2.0.6",
    "ts-prune": "^0.10.3",
    "vite": "4.4.6",
    "vite-plugin-checker": "0.6.1",
    "vite-plugin-svgr": "3.2.0",
    "vite-tsconfig-paths": "4.2.0"
  },
  "packageManager": "yarn@3.6.1"
}
