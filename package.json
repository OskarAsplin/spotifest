{
  "name": "spotifest",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.15.0",
    "@mui/styles": "^5.15.0",
    "@mui/x-date-pickers": "^6.18.5",
    "@tanstack/react-query": "^5.14.0",
    "@tanstack/react-query-devtools": "^5.14.0",
    "@tanstack/react-router": "^1.0.0",
    "@tanstack/react-virtual": "^3.0.1",
    "@tanstack/router-devtools": "^1.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/async": "^3.2.24",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^29.5.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-image-gallery": "^1.2.4",
    "@types/react-swipeable-views": "^0.13.5",
    "copy-to-clipboard": "^3.3.3",
    "countries-list": "^3.0.6",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "i18next": "^23.7.11",
    "lodash-es": "^4.17.21",
    "madge": "^6.1.0",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-cookie-consent": "^9.0.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-i18next": "^13.5.0",
    "react-image-gallery": "^1.3.0",
    "react-intl": "^6.5.5",
    "react-player": "2.13.0",
    "react-share": "^5.0.3",
    "react-swipeable-views": "^0.14.0",
    "sass": "^1.69.5",
    "spotify-web-api-js": "^1.5.2",
    "typescript": "^5.3.3",
    "zustand": "^4.4.7"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "find-circular-dependencies": "madge --circular ./src/index.tsx",
    "find-unused-exports": "ts-prune",
    "find-dead-code": "ts-prune | grep -v \"(used in module)\" || true"
  },
  "type": "module",
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "madge": {
    "detectiveOptions": {
      "tsx": {
        "skipTypeImports": true
      },
      "ts": {
        "skipTypeImports": true
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/react": "^7.6.5",
    "@storybook/react-vite": "^7.6.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromatic": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "^3.2.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.6.5",
    "storybook-react-i18next": "^2.0.9",
    "ts-prune": "^0.10.3",
    "vite": "^5.0.13",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "packageManager": "yarn@3.6.1"
}
